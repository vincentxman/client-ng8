{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.bak": true,
    "**/*.spec.ts": true,
    "**/node_modules": true,
    "**/e2e": true,
    ".idea": true,
    "dist": true,
    ".vscode": false,
    "yarn-error.log": true,
    "yarn.lock": true,
    "**/browserslist": true,
    "**/cmd.exe.lnk": true,
    "*.lnk": true,
    "cmd.exe.lnk": true,
    "**/*.lnk": true,
    "out-tsc": true,
    "site": true,
    "components": true
  },
  "git.ignoreLimitWarning": true,
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "editor.detectIndentation": false,
  "editor.tabSize": 4
}
