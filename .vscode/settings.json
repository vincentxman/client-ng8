{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.bak": true,
        "**/*.spec.ts": true,
        "**/node_modules": true,
        "**/e2e": true,
        ".idea": true,
        "dist": false,
        ".vscode": true,
        "yarn-error.log": true,
        "yarn.lock": true,
        "**/browserslist": true,
        "**/cmd.exe.lnk": true,
        "*.lnk": true,
        "cmd.exe.lnk": true,
        "**/*.lnk": true
    },
    "explorerExclude.backup": null,
    "git.ignoreLimitWarning": true,
    "javascript.preferences.importModuleSpecifier": "relative",
    "typescript.preferences.importModuleSpecifier": "relative"
}
